#include<stdio.h>
#include<string.h>
#include<ctype.h>
#include "set.h"
int n,type;
char word[100],definition[100];
int main()
{
	FILE *fi = fopen("test","r");
	set_node *set;
	set_init(&set);
	fscanf(fi,"%d", &n);       // n represents the number of words we're going to run through the program
	for (int i = 1;i <= n;i++)
	{
		fscanf(fi, "%d", &type);   // The command type
		if (type == 1)       // Type 1 is for Inserting a word into the dictionary
		{
			fscanf(fi, "%s", word);
			fgetc(fi);
			fgets(definition, 100, fi);
			if (!(set_search(set, word, strlen(word) + 1, strcmp)))
			{
				set_insert(&set, word, strlen(word) + 1, strcmp);
				FILE *f = fopen(word, "w");
				fprintf(f, "%s", definition);
				fclose(f);
			}
		}
		else
			if (type == 2)
			{
				fscanf(fi, "%d", &type);
				if (set_search(set, word, strlen(word) + 1, strcmp))
				{

				}
			}
			else
				if (type == 3)
				{

				}
				else
					if (type == 4)
					{

					}


	}
	return 0;
}
